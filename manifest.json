{
   "manifest_version": 2,
   "name": "Adjust Video Brightness",
   "description": "Adjust Video Brightness & Playback Rate",
   "content_scripts": [ {
      "all_frames": true,
      "run_at": "document_end",
      "js": [ "contentscript.js" ],
      "matches": [ "<all_urls>" ]
   } ],
   "web_accessible_resources": [
      "icon24.png",
      "close.png"
   ],
   "icons": {
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
   },
   "version": "0.5"
}
